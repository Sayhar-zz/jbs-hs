<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="org.jbs.happysad" android:versionCode="1" android:versionName="1.0">
	<uses-sdk android:minSdkVersion="8" />
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
	<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
	<uses-permission android:name="android.permission.INTERNET"/> 
	<uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

	<application android:icon="@drawable/happysad"
		android:label="@string/app_name">
		<uses-library android:name="com.google.android.maps" />
        <activity android:name=".Prompt"
                  android:label="@string/app_name">
           <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".More"
           		  android:label="@string/more_name"	
           		  android:noHistory="true" >		
        </activity>
        <activity android:name=".PersonalMap"
        	      android:label="@string/personalMapTitle">
        </activity>
        <activity android:name=".Dashboard"
        	      android:label="@string/dashboard_name" >
        </activity>
        <activity android:name=".History"
        	      android:label="@string/history_name" >
        </activity>
        <activity android:name=".Prefs"
        	      android:label="@string/settings_title">
        </activity>
        
        <activity 
			android:name=".FindMeHappiness" 
			android:label="@string/fmh_name">
		</activity>

	</application>

</manifest>
